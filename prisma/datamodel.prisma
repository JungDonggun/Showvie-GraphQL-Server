type User {
  id: ID! @id
  identity: String! @unique
  nickname: String!
  password: String! @encrypt
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  movieReviews: [MovieReview]
}

type MovieReview {
  id: ID! @id
  title: String!
  rating: Int! @default(value: 0)
  review: String
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Movies {
  id: ID! @id
  movieNm: String!
  movieNmEn: String!
  prdtYear: String!
  openDt: String!
  prdtStatNm: String!
  nationAlt: String!
  genreAlt: String!
  repNationNm: String!
  directors: String!
}
