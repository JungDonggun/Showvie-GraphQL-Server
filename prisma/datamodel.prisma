type User {
  id: ID! @id
  identity: String! @unique
  nickname: String!
  password: String! @encrypt
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  movieReviews: [MovieReview]
}

type MovieReview {
  id: ID! @id
  title: String!
  rating: Int! @default(value: 0)
  review: String
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
